package de.monticore.lang.montiarc;

grammar AJava extends de.monticore.lang.montiarc.MontiArcAutomaton {
  
  start MACompilationUnit;
  
  ComponentInitialization implements Element = "init" "{" VariableInitialization+ "}";
  
  VariableInitialization = QualifiedName "=" value:Expression ";";
    
  AJavaDefinition implements BehaviorEmbedding = "compute" Name "{" BlockStatement+ "}";
    
}
