package demo.runningexample;

Comp0 {
  check mandatory effect: in1 => out1;
  check no effect: in1 => out2;
}

Comp1 { 
  ensure mandatory effect: in1 => out2;
  ensure mandatory effect: in2 => out1;
  ensure no effect: in1 => out1;
}

Comp2 {
  ensure mandatory effect: in1 => out1;
  ensure mandatory effect: in1 => out3;
  ensure mandatory effect: in2 => out1;
  ensure mandatory effect: in2 => out2;
  ensure no effect: in1 => out2;
}

Comp3 {
  ensure mandatory effect: in1 => out1;
  ensure mandatory effect: in2 => out1;
  ensure no effect: in1 => out2;
  ensure no effect: in2 => out2;
}