package valid.namecompletion;

import valid.namecompletion.Types.*;

automaton CompleteCD {
	input Distance distance;
	output Command command;
	
	state Okay, Stopped;
	initial Okay / {Command.createStop()};
	
	Okay -> Stopped {Distance.CLOSE} / {Command.createStop()};
	// What would be the purpose of object-pattern matching for inputs?
  // will be translated to == on object code - whatever this implies	
}