package valid.namecompletion;

import java.lang.*;
import java.util.*;

automaton CompleteComplexJava {
  input Boolean createList;
  output Object dataStructure;
  variable Integer occurrences = 0;
  
  state Okay;
  
  initial Okay / {new java.util.ArrayList()}; 
  // should work
  
  Okay {true}  / {dataStructure = new java.util.ArrayList(), occurrences + 1};
  // first part should work
  // second part should be ambiguous
  
  Okay {false} / {dataStructure = new java.util.HashSet(), occurrences * 2};
  // first part should work
  // second part should be ambiguous
}