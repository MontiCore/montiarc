package valid.namecompletion;

import java.util.*;
import java.lang.*;

automaton CompleteComplexJavaWithErrors {
  input Boolean createList;
  output Collection dataStructure;
  variable Integer occurrences = new Integer(3);  // this should not be a problem!
  variable Integer number = 3;                    // this should not be a problem either!
  
  state Okay;
  initial Okay / {true};
  // 1x Automaton has no sink for type boolean
  
  Okay {new ArrayList()}  / {new HashSet(), new String(3)};
  // 1x Automaton has no source for type ArrayList
  // 1x Automaton has no sink for type HashSet
  // 1x Automaton has not sink for type String
  
}