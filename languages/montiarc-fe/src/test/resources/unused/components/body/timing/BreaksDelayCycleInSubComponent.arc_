package components.body.timing;

import components.body.subcomponents._subcomponents.HasStringInputAndOutput;

/*
 * Valid model.
 */
component BreaksDelayCycleInSubComponent {

    port
        in String sIn,
        out String sOut;

    component HasStringInputAndOutput a;
    component HasTwoStringInAndOneStringOut c;
    
    component UsingCompsWithTwoPortsAndWithAndWithoutDelay using;
    
    connect sIn -> c.sIn1;
    connect c.sOut -> a.pIn;
    connect a.pOut -> sOut, using.sIn;
    connect using.sOut -> c.sIn2;
    
}
