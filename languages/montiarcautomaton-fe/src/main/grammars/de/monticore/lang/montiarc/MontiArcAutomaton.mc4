package de.monticore.lang.montiarc;

grammar MontiArcAutomaton extends de.monticore.lang.montiarc.MontiArcBehavior, de.monticore.automaton.IOAutomatonJava {
  
  // use MA compilation unit as start
  start MACompilationUnit;
    
  // IO-Automaton body is used as behavior embedding  
  AutomatonDefinition implements BehaviorEmbedding = "automaton" Name "{" AutomatonBody "}";  
   
}
